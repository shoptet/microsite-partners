{% extends "base.twig" %}

{% block breadcrumb %}
	<div class="row-breadcrumb)">
		<div class="container">
			{% include 'breadcrumb.twig' %}
		</div>
	</div>
{% endblock %}

{% block content %}
<div class="row-404">
	<div class="container">
		<h1 class="h1 text-primary">{{ wp_title }}</h1>

		{% if options.themeNotFoundText %}
			<div class="lead mt-4">
				{{ options.themeNotFoundText }}
			</div>
		{% endif %}

		<img
			class="row-404-image mt-4"
			src="{{ "shoptetrix-meditate.png"|static_assets }}"
			width="300"
			alt="Shoptetrix"
		>

	</div>
</div>
{% endblock %}
