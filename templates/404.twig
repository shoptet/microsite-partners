{% extends "base.twig" %}

{% block breadcrumb %}
	<div class="row-breadcrumb)">
		<div class="container">
			{% include 'breadcrumb.twig' %}
		</div>
	</div>
{% endblock %}

{% block content %}
<div class="row-404">
	<div class="container">
		<h1 class="h1 text-primary">{{ wp_title }}</h1>

		{% if options.themeNotFoundText %}
			<div class="lead mt-4">
				{{ options.themeNotFoundText }}
			</div>
		{% endif %}

	</div>
</div>
{% endblock %}
